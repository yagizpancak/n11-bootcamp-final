FROM eclipse-temurin:17-jdk-jammy AS base
WORKDIR /app
COPY log-service/.mvn/ .mvn
COPY log-service/mvnw log-service/pom.xml ./
RUN ./mvnw dependency:resolve
COPY log-service/src ./src


FROM base AS build
RUN ./mvnw package


FROM eclipse-temurin:17-jre-jammy AS production
EXPOSE 8082
COPY --from=build /app/target/log-service-*.jar /log-service.jar
CMD ["java", "-Djava.security.egd=file:/dev/./urandom", "-jar", "/log-service.jar"]